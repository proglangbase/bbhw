DYALOG=$(which dyalogscript.bash)
if [ "$DYALOG" = "" ]; then
    echo "Dyalog must be installed."
    echo "A free non-commerical version can be obtained at"
    echo "https://dyalog.com/download-zone.htm"
    exit 1
fi
$DYALOG bbhw.apls $@
